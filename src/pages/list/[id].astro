---
import Layout from "../../components/Layout.astro";

const { id: groupId } = Astro.params;
const response = await fetch(`http://localhost:3000/api/product/${groupId}`);
const group = await response.json();
---
<Layout title={group.name} color="white" >
	Group name: {group.name} <br />
	Group id: {group.id}
</Layout>
